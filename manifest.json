{
  "name" : "Rico单词本-收录网页中的单词进我的单词本",
  "version" : "1.0.1",
  "description" : "Get more words,all in one bowl! 一个可以将网页中的生词收录进我的单词本的小应用",
  "background" : { "scripts": ["background.js"] },
  "content_scripts": [ 
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["js/jquery.js","js/select.js"]       
    }
  ],
  "permissions" : [
    "contextMenus",
    "notifications",
    "declarativeContent",
    "storage",
    "http://*/*",
    "https://*/*",
    "tabs"
   ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16" : "img/16.png",
        "48" : "img/48.png",
        "128" : "img/128.png"
      }
  },
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "img/16.png",
    "48" : "img/48.png",
    "128" : "img/128.png"
  },
  "manifest_version": 2
}
